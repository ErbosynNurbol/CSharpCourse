<main>
    <div class="container-fluid">
        <form action="/admin/uploadmedias" method="post" enctype="multipart/form-data" id="form-upload">
             <div class="md-form">
                   <input type="file" id="mFile" name="mFiles" accept="image/jpeg,application/pdf,audio/mpeg,video/mp4,text/plain,image/png,image/gif" multiple>
               </div>
               <div class="md-form">
                    <div class="md-progress">
                                <div class="progress-bar bg-info" id="uploadProgressBar" role="progressbar" style="width: 0%" aria-valuenow="50" aria-valuemin="0"
                                       aria-valuemax="100"></div>
                          </div>
               </div>
               <div class="md-form">
                    <button class="btn btn-info" type="submit">Upload</button>
               </div>
        </form>
    </div>
</main>

